{
   "name": "@mariozechner/jailjs",
   "version": "0.1.0",
   "description": "JavaScript AST interpreter for sandboxed execution in browsers and Node.js",
   "author": "Mario Zechner",
   "type": "module",
   "main": "./dist/index.js",
   "module": "./dist/index.js",
   "types": "./dist/index.d.ts",
   "files": [
      "dist",
      "README.md"
   ],
   "exports": {
      ".": {
         "types": "./dist/index.d.ts",
         "import": "./dist/index.js"
      },
      "./parser": {
         "types": "./dist/parser.d.ts",
         "import": "./dist/parser.js"
      },
      "./dist/*": "./dist/*"
   },
   "scripts": {
      "build": "tsc && cd example/web && npm run build && cd ../node && npm run build && cd ../chrome-extension && npm run build",
      "dev": "concurrently -n tsc,web,ext -c cyan,green,magenta \"npm run dev:tsc\" \"npm run dev:web\" \"npm run dev:ext\"",
      "dev:tsc": "tsc --watch --preserveWatchOutput",
      "dev:web": "cd example/web && npm run dev",
      "dev:ext": "cd example/chrome-extension && npm run dev",
      "check": "biome check . --write && tsc --noEmit && cd example/web && npm run check && cd ../node && npm run check",
      "prepare": "husky",
      "test": "vitest",
      "test:ui": "vitest --ui"
   },
   "dependencies": {
      "@babel/parser": "^7.26.10",
      "@babel/preset-env": "^7.28.3",
      "@babel/standalone": "^7.28.4",
      "@babel/types": "^7.26.10"
   },
   "devDependencies": {
      "@biomejs/biome": "^2.2.4",
      "@types/babel__traverse": "^7.20.6",
      "@types/node": "^24.5.2",
      "@vitest/ui": "^3.2.4",
      "concurrently": "^9.2.1",
      "esbuild": "^0.25.10",
      "happy-dom": "^15.11.7",
      "husky": "^9.1.7",
      "typescript": "^5.9.2",
      "vitest": "^3.2.4"
   }
}
